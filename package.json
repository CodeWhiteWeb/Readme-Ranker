{
  "name": "@CodeWhiteWeb/readme-ranker",
  "version": "1.0.2",
  "description": "A CLI tool to analyze and rank README files for projects, providing actionable suggestions for improvement.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "readme-ranker": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeWhiteWeb/Readme-Ranker.git"
  },
  "homepage": "https://github.com/CodeWhiteWeb/Readme-Ranker#readme",
  "bugs": {
    "url": "https://github.com/CodeWhiteWeb/Readme-Ranker/issues"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "rev": "tsc --incremental",
    "dev": "npx ts-node src/index.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "readme",
    "analyzer",
    "ranker",
    "npm",
    "cli",
    "documentation",
    "quality"
  ],
  "author": "CodeWhiteWeb",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^9.0.0",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
